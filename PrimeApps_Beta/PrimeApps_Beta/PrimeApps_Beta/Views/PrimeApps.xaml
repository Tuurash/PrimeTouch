<?xml version="1.0" encoding="utf-8" ?>
<MasterDetailPage xmlns="http://xamarin.com/schemas/2014/forms"
             xmlns:x="http://schemas.microsoft.com/winfx/2009/xaml"
             x:Class="PrimeApps_Beta.Views.PrimeApps"
             xmlns:pages="clr-namespace:PrimeApps_Beta.Views" 
             NavigationPage.HasNavigationBar="False">
    <MasterDetailPage.Master>
        <pages:PrimeAppsMaster x:Name="MasterPage" />
    </MasterDetailPage.Master>
    <MasterDetailPage.Detail>
        <NavigationPage BarTextColor="#fbfbff">
                
            <x:Arguments>
                <pages:PrimeAppsDetail />
            </x:Arguments>
        </NavigationPage>
    </MasterDetailPage.Detail>
</MasterDetailPage>